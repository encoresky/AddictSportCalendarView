<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.calendarsampleapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.calendarsampleapp.MainActivity">

        </activity>

        <activity
            android:name="com.android.calendarsampleapp.CalendarActivity"
            android:label="Standard calendar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.android.calendarsampleapp.OneDayPickerActivity"
            android:label="One day picker" />

        <activity
            android:name="com.android.calendarsampleapp.ManyDaysPickerActivity"
            android:label="Many days picker" />

        <activity
            android:name="com.android.calendarsampleapp.RangePickerActivity"
            android:label="Range picker" />
    </application>

</manifest>